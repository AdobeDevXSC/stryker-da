{
  "groups": [
    {
      "title": "Default Content",
      "id": "default",
      "components": [
        {
          "title": "Text",
          "id": "text",
          "plugins": {
            "da": {
              "name": "text",
              "type": "text"
            }
          }
        },
        {
          "title": "Image",
          "id": "image",
          "plugins": {
            "da": {
              "name": "image",
              "type": "image"
            }
          }
        }
      ]
    },
    {
      "title": "Sections",
      "id": "sections",
      "components": [
        {
          "title": "Section",
          "id": "section",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>"
            }
          },
          "filter": "section",
          "model": "section"
        }
      ]
    },
    {
      "title": "Blocks",
      "id": "blocks",
      "components": [
        {
          "title": "Cards",
          "id": "cards",
          "model": "cards",
          "filter": "cards",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 2
            }
          }
        },
        {
          "title": "Card",
          "id": "card",
          "model": "card",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 2,
              "fields": [
                {
                  "name": "image",
                  "selector": "div:nth-child(1)>picture>img[src]"
                },
                {
                  "name": "imageAlt",
                  "selector": "div:nth-child(1)>picture>img[alt]"
                },
                {
                  "name": "text",
                  "selector": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Carousel",
          "id": "carousel",
          "model": "carousel",
          "filter": "carousel",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 2
            }
          }
        },
        {
          "title": "Carousel Slide",
          "id": "carousel-item",
          "model": "carousel-item",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 2,
              "fields": [
                {
                  "name": "backgroundImage",
                  "selector": "div:nth-child(1)>picture>img[src]"
                },
                {
                  "name": "backgroundImageAlt",
                  "selector": "div:nth-child(1)>picture>img[alt]"
                },
                {
                  "name": "text",
                  "selector": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Columns",
          "id": "columns",
          "model": "columns",
          "filter": "columns",
          "plugins": {
            "da": {
              "name": "columns",
              "rows": 1,
              "columns": 2,
              "behaviour": "columns"
            }
          }
        },
        {
          "title": "Columns Row",
          "id": "columns-row",
          "model": "columns-row",
          "filter": "columns-row",
          "plugins": {
            "da": {
              "name": "columns-row",
              "behaviour": "columns-row"
            }
          }
        },
        {
          "title": "Columns Cell",
          "id": "columns-cell",
          "model": "columns-cell",
          "filter": "columns-cell",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>",
              "behaviour": "columns-cell"
            }
          }
        },
        {
          "title": "Fragment",
          "id": "fragment",
          "model": "fragment",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "name": "fragmentUrl",
                  "selector": "div>div>p>a[href]"
                },
                {
                  "name": "fragmentUrlText",
                  "selector": "div>div>p>a"
                }
              ]
            }
          }
        },
        {
          "title": "Hero",
          "id": "hero",
          "model": "hero",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "name": "image",
                  "selector": "div>div>picture>img[src]"
                },
                {
                  "name": "alt",
                  "selector": "div>div>picture>img[alt]"
                },
                {
                  "name": "text",
                  "selector": "div>div>h1"
                }
              ]
            }
          }
        },
        {
          "title": "Media Promo Banner",
          "id": "media-promo-banner",
          "model": "media-promo-banner",
          "plugins": {
            "da": {
              "rows": 4,
              "columns": 2,
              "fields": [
                {
                  "name": "image",
                  "selector": "div:nth-child(1)>div:nth-child(1)>picture:nth-child(1)>img:nth-child(3)[src]"
                },
                {
                  "name": "alt",
                  "selector": "div:nth-child(1)>div:nth-child(1)>picture:nth-child(1)>img:nth-child(3)[alt]"
                },
                {
                  "name": "description",
                  "selector": "div:nth-child(2)"
                },
                {
                  "name": "url",
                  "selector": "div:nth-child(3)>div:nth-child(1)>p:nth-child(1)>a:nth-child(1)[href]"
                }
              ]
            }
          }
        },
        {
          "title": "Product List Page",
          "id": "product-list-page",
          "model": "product-list-page",
          "plugins": {
            "da": {
              "type": "key-value-block"
            }
          }
        },
        {
          "title": "Product Recommendations",
          "id": "product-recommendations",
          "model": "product-recommendations",
          "plugins": {
            "da": {
              "type": "key-value-block"
            }
          }
        },
        {
          "title": "Quote",
          "id": "quote",
          "plugins": {
            "da": {
              "name": "quote",
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "Search",
          "id": "search",
          "plugins": {
            "da": {
              "name": "search",
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "Tabs",
          "id": "tabs",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"tabs\"><div><div><p></p></div><div></div></div></div>"
            }
          }
        },
        {
          "title": "Tabs Item",
          "id": "tabs-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><p></p></div><div></div></div>"
            }
          }
        },
        {
          "title": "Video",
          "id": "video",
          "model": "video",
          "plugins": {
            "da": {
              "name": "video",
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "name": "videoUrl",
                  "selector": "div:nth-child(1)>div:nth-child(1)>p:nth-child(1)>a:nth-child(1)[href]"
                },
                {
                  "name": "text",
                  "selector": "div:nth-child(2)"
                }
              ]
            }
          }
        }
      ]
    }
  ]
}